# запуск терминала
super + Escape
    alacritty

# запуск редактора
super + e
    st -e fish -i -c nvim

# запуск dmenu
super + p
    dmenu_run -fn "Terminus:size=16"

# вставить текст из буфера
shift+Insert
    clipmenu && sleep 0.5 && xclip -o | xdotool type --clearmodifiers --file -

# вставить текст из сниппета
alt + Down
    snip load

super + semicolon
    keynav
