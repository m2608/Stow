# запуск терминала
super + Escape
    st

# запуск редактора
super + e
    st -e fish -i -c nvim

# запуск dmenu
super + p
    dmenu_run -fn "Terminus:size=16" -nb '#202020' -nf '#a0a0a0' -sb '#606060' -sf '#e0e0e0'

# вставить текст из буфера
shift+Insert
    clipmenu && sleep 0.5 && xclip -o | xdotool type --clearmodifiers --file -

# вставить текст из сниппета
alt + Down
    snip load

super + semicolon
    keynav
