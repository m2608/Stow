set PATH \
    $HOME/.local/bin \
    $HOME/.cargo/bin \
    $HOME/.npm-global/bin \
    $HOME/.gocode/bin \
    /usr/local/bin \
    /usr/local/sbin \
    /usr/bin \
    /usr/sbin \
    /bin \
    /sbin \
    /usr/lib/jvm/default-jdk/bin \
    /usr/lib/jvm/default-jre/bin

setenv PAGER nvimpager
setenv NODE_PATH (npm root -g)

alias ls lsd
alias nnn "PAGER='nvimpager -p' "(which nnn)

zoxide init fish | source

